/dts-v1/;

#include <arm64/rockchip/rk3588.dtsi>
#include <zephyr/dt-bindings/gpio/gpio.h>



/ {
	model = "OrangePi 5 Plus";
	compatible = "rockchip,rk3588";
	
	chosen {
		zephyr,sram = &sram0;
		zephyr,console = &uart2;
		zephyr,shell-uart = &uart2;
	};

};



&uart2 {
	status = "okay";
};

&gmac0 {
	status = "okay";
	local-mac-address = [00 30 18 35 58 58];
	pinctrl-0 = <&gmac0_rgmii_pins>;
	phy-connection-type = "rgmii";
	rockchip,tx-delay-ps = <1800>;
	rockchip,rx-delay-ps = <1800>;
};
